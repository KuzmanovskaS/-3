#include <iostream>
#include <bits/stdc++.h>
using namespace std;

bool visited[100];
vector<int> niz[100];  //grafot
vector <int> pateka;

void dfs( int start)
{
    visited[start]=true;

    pateka.push_back(start);

    vector<int>::iterator i;  //da gi izmineme sosedite

    for(i=niz[start].begin();i!=niz[start].end();i++)
    {
        if(visited[*i]==false)
            dfs(*i);
    }
}


int main()
{
    int v,a,b,t,start,endd;
    cout<<"Vnesete broj na teminja vo grafot"<<endl;
    cin>>v;
    string s;
    cout<<"Vnesete parovi od sosedni teminja. "<<endl;

    cin>>s;

    while(s!="KRAJ")
    {
        a=stoi(s.substr(0,1));
        b=stoi(s.substr(2,1));
        niz[a].push_back(b);
        cin>>s;
    }

    cout<<"Vnesete pocetok i kraj na patekata"<<endl;
    cin>>start>>endd;
    cout<<"Patekata e: ";
    dfs(start);

    for(int i=0;i<pateka.size();i++) //??????
    {
        cout<<pateka[i]<<" ";
        if(pateka[i]==endd)
            break;
    }

    return 0;
}



